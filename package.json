{
  "name": "quantumtask-ai-receptionist",
  "version": "1.0.0",
  "description": "Quantumtask AI-powered business receptionist with intelligent chat and dynamic content delivery",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "docker:build": "docker build -t ai-business-receptionist .",
    "docker:run": "docker run -p 8080:80 ai-business-receptionist",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up -d --build",
    "docker:logs": "docker-compose logs -f app",
    "clean": "rm -rf dist node_modules package-lock.json",
    "install:clean": "npm run clean && npm install"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.0",
    "@heroicons/vue": "^2.0.0",
    "axios": "^1.6.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.2",
    "vite": "^5.0.0",
    "vue-tsc": "^2.2.12"
  }
}
